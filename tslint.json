{
    "defaultSeverity": "warn",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "semicolon": [
            true,
            "never"
        ],
        "object-literal-sort-keys": false,
        "quotemark": [
            true,
            "single",
            "avoid-escape",
            "avoid-template"
        ],
        "member-access": [
            true,
            "no-public"
        ]
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "**/*.js"
        ]
    }
}
